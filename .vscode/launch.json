{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug Rust",
          "type": "lldb",
          "request": "launch",
          "program": "${workspaceFolder}/target/debug/${workspaceFolderBasename}",
          "args": [],
          "cwd": "${workspaceFolder}",
          "stopOnEntry": false,
          "sourceLanguages": ["rust"],
          "env": {
              "MODE": "DEVEL",
              "DEVFRONTPORT": "2081",
              "SCRIPTSFOLDER": "rest-examples",
              "PORT": "2080"
          },
          "initCommands": [
              "settings set target.max-string-summary-length 1000"
          ]
      }
  ]
}